<UserControl x:Class="BadassUniverse_MapEditor.Views.Elements.PropertyElementView"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:BadassUniverse_MapEditor.Views.Elements"
             mc:Ignorable="d" 
             d:DesignHeight="450" d:DesignWidth="800">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="75"/>
            <RowDefinition Height="75"/>
            <RowDefinition Height="75"/>
            <RowDefinition Height="75"/>
        </Grid.RowDefinitions>

        <Frame Grid.Row="0" Source="SubElements/PropertySubElementButton.xaml"/>
        <Frame Grid.Row="1" Source="SubElements/PropertySubElementCheckBox.xaml"/>
        <Frame Grid.Row="2" Source="SubElements/PropertySubElementComboBox.xaml"/>
        <Frame Grid.Row="3" Source="SubElements/PropertySubElementTextBox.xaml"/>
    </Grid>
</UserControl>
